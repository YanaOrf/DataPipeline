    "import pandas as pd\n",
    "import gspread \n",
    "\n",
    "def get_data():\n",
    "    gc=gspread.service_account(filename=\"./service_account.json\")\n",
    "    wks=gc.open(\"Tesla price\").sheet1\n",
    "    df=pd.DataFrame(wks.get_all_records())\n",
    "    return df\n",
    "get_data()\n"
   ]
